{
  "name": "boilerplate-fullstack-express-react",
  "version": "2.0.0",
  "description": "boilerplate for create web app using express js and react js",
  "main": "index.js",
  "scripts": {
    "client-build": "cd client && npm run build",
    "client-dev": "npm run serve --prefix client",
    "server-dev": "nodemon server.js",
    "development": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-prebuild": "npm install npm@latest -g && cd client && npm install --production=false",
    "heroku-postbuild": "npm run client-build"
  },
  "keywords": [
    "express",
    "api"
  ],
  "author": "basri basren",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-rid": "0.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "csurf": "^1.9.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.17",
    "morgan": "^1.9.1",
    "promise": "^8.0.2",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui": "^3.22.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.10",
    "prettier": "^1.17.0"
  }
}
