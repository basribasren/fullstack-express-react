{
	"presets": [
		["@babel/preset-env"],
	],
	"plugins": [
		["module-resolver",
		{
			"extensions": [".ios.js", ".android.js", ".js", ".json", "jsx"],
			"alias":
			{
				"@": "./server/",
				"@config": "./server/config",
				"@helpers": "./server/helpers",
				"@modules": "./server/modules",
			}
		}]
	]
}